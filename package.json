  "description": "Hansei Sales Intelligence Backend API - Optimized Performance Edition",
  "main": "app.js",
  "scripts": {
    "start": "node scripts/create-schema.js && (node scripts/import-excel.js || echo 'Import skipped - running without initial data') && node app.js",
    "dev": "nodemon app.js",
    "build": "echo 'No build step required'",
